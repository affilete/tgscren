# Telegram Bot Configuration
# Get your bot token from @BotFather on Telegram
BOT_TOKEN=your_bot_token_here

# Your Telegram User ID (required for bot authorization)
# Get it from @userinfobot on Telegram
OWNER_USER_ID=123456789

# Default Chat ID for sending alerts
# For groups/channels, use the format: -1001234567890
# For private chats, use your user ID
DEFAULT_CHAT_ID=-1001234567890

# Optional: Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
